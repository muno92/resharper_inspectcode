{
  "name": "resharper_inspectcode",
  "version": "1.12.2",
  "private": true,
  "description": "Run ReSharper Inspection action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muno92/resharper_inspectcode.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "htmlparser2": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.14.0",
    "eslint-plugin-github": "^5.0.0",
    "eslint-plugin-jest": "^28.0.0",
    "jest": "^29.0.0",
    "jest-circus": "^29.0.0",
    "jest-extended": "^4.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.4.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
